ACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Seller.vue","sourceRoot":"src/components","sourcesContent":["<!-- ÂïÜÂÆ∂ÈîÄÈáèÁªüËÆ°ÁöÑÊ®™ÂêëÊü±Áä∂Âõæ -->\n<template>\n  <div class=\"com-container\">\n    <div class=\"com-chart\" ref=\"seller_ref\"></div>\n  </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex'\nexport default {\n  data () {\n    return {\n      chartInstance: null,\n      allData: null, // ÊúçÂä°Âô®ËøîÂõûÁöÑÊï∞ÊçÆ\n      currentPage: 1, // ÂΩìÂâçÊòæÁ§∫ÁöÑÈ°µÊï∞\n      totalPage: 0, // ‰∏ÄÂÖ±ÊúâÂ§öÂ∞ëÈ°µ\n      timerId: null // ÂÆöÊó∂Âô®ÁöÑÊ†áËØÜ\n    }\n  },\n  created () {\n    // Âú®ÁªÑ‰ª∂ÂàõÂª∫ÂÆåÊàê‰πãÂêé ËøõË°åÂõûË∞ÉÂáΩÊï∞ÁöÑÊ≥®ÂÜå\n    this.$socket.registerCallBack('sellerData', this.getData)\n  },\n  mounted () {\n    this.initChart()\n    // this.getData()\n    this.$socket.send({\n      action: 'getData',\n      socketType: 'sellerData',\n      chartName: 'seller',\n      value: ''\n    })\n    window.addEventListener('resize', this.screenAdapter)\n    // Âú®È°µÈù¢Âä†ËΩΩÂÆåÊàêÁöÑÊó∂ÂÄô, ‰∏ªÂä®ËøõË°åÂ±èÂπïÁöÑÈÄÇÈÖç\n    this.screenAdapter()\n  },\n  destroyed () {\n    clearInterval(this.timerId)\n    // Âú®ÁªÑ‰ª∂ÈîÄÊØÅÁöÑÊó∂ÂÄô, ÈúÄË¶ÅÂ∞ÜÁõëÂê¨Âô®ÂèñÊ∂àÊéâ\n    window.removeEventListener('resize', this.screenAdapter)\n    this.$socket.unRegisterCallBack('sellerData')\n  },\n  methods: {\n    // ÂàùÂßãÂåñechartInstanceÂØπË±°\n    initChart () {\n      this.chartInstance = this.$echarts.init(this.$refs.seller_ref, this.theme)\n      // ÂØπÂõæË°®ÂàùÂßãÂåñÈÖçÁΩÆÁöÑÊéßÂà∂\n      const initOption = {\n        title: {\n          text: 'üíõ ÂïÜÂÆ∂ÈîÄÂîÆÁªüËÆ°',\n          left: 20,\n          top: 20\n        },\n        grid: {\n          top: '20%',\n          left: '3%',\n          right: '6%',\n          bottom: '3%',\n          containLabel: true // Ë∑ùÁ¶ªÊòØÂåÖÂê´ÂùêÊ†áËΩ¥‰∏äÁöÑÊñáÂ≠ó\n        },\n        xAxis: {\n          type: 'value'\n        },\n        yAxis: {\n          type: 'category'\n        },\n        tooltip: {\n          trigger: 'axis',\n          axisPointer: {\n            type: 'line',\n            z: 0,\n            lineStyle: {\n              color: '#2D3443'\n            }\n          }\n        },\n        series: [\n          {\n            type: 'bar',\n            label: {\n              show: true,\n              position: 'right',\n              textStyle: {\n                color: 'white'\n              }\n            },\n            itemStyle: {\n              // ÊåáÊòéÈ¢úËâ≤Ê∏êÂèòÁöÑÊñπÂêë\n              // ÊåáÊòé‰∏çÂêåÁôæÂàÜÊØî‰πã‰∏ãÈ¢úËâ≤ÁöÑÂÄº\n              color: new this.$echarts.graphic.LinearGradient(0, 0, 1, 0, [\n                // ÁôæÂàÜ‰πã0Áä∂ÊÄÅ‰πã‰∏ãÁöÑÈ¢úËâ≤ÂÄº\n                {\n                  offset: 0,\n                  color: '#5052EE'\n                },\n                // ÁôæÂàÜ‰πã100Áä∂ÊÄÅ‰πã‰∏ãÁöÑÈ¢úËâ≤ÂÄº\n                {\n                  offset: 1,\n                  color: '#AB6EE5'\n                }\n              ])\n            }\n          }\n        ]\n      }\n      this.chartInstance.setOption(initOption)\n      // ÂØπÂõæË°®ÂØπË±°ËøõË°åÈº†Ê†á‰∫ã‰ª∂ÁöÑÁõëÂê¨\n      this.chartInstance.on('mouseover', () => {\n        clearInterval(this.timerId)\n      })\n      this.chartInstance.on('mouseout', () => {\n        this.startInterval()\n      })\n    },\n    // Ëé∑ÂèñÊúçÂä°Âô®ÁöÑÊï∞ÊçÆ\n    getData (ret) {\n      // http://127.0.0.1:8888/api/seller\n      // const { data: ret } = await this.$http.get('seller')\n      this.allData = ret\n      // ÂØπÊï∞ÊçÆÊéíÂ∫è\n      this.allData.sort((a, b) => {\n        return a.value - b.value // ‰ªéÂ∞èÂà∞Â§ßÁöÑÊéíÂ∫è\n      })\n      // ÊØè5‰∏™ÂÖÉÁ¥†ÊòæÁ§∫‰∏ÄÈ°µ\n      this.totalPage = this.allData.length % 5 === 0 ? this.allData.length / 5 : this.allData.length / 5 + 1\n      this.updateChart()\n      // ÂêØÂä®ÂÆöÊó∂Âô®\n      this.startInterval()\n    },\n    // Êõ¥Êñ∞ÂõæË°®\n    updateChart () {\n      const start = (this.currentPage - 1) * 5\n      const end = this.currentPage * 5\n      const showData = this.allData.slice(start, end)\n      const sellerNames = showData.map((item) => {\n        return item.name\n      })\n      const sellerValues = showData.map((item) => {\n        return item.value\n      })\n      const dataOption = {\n        yAxis: {\n          data: sellerNames\n        },\n        series: [\n          {\n            data: sellerValues\n          }\n        ]\n      }\n      this.chartInstance.setOption(dataOption)\n    },\n    startInterval () {\n      if (this.timerId) {\n        clearInterval(this.timerId)\n      }\n      this.timerId = setInterval(() => {\n        this.currentPage++\n        if (this.currentPage > this.totalPage) {\n          this.currentPage = 1\n        }\n        this.updateChart()\n      }, 3000)\n    },\n    // ÂΩìÊµèËßàÂô®ÁöÑÂ§ßÂ∞èÂèëÁîüÂèòÂåñÁöÑÊó∂ÂÄô, ‰ºöË∞ÉÁî®ÁöÑÊñπÊ≥ï, Êù•ÂÆåÊàêÂ±èÂπïÁöÑÈÄÇÈÖç\n    screenAdapter () {\n      // console.log(this.$refs.seller_ref.offsetWidth)\n      const titleFontSize = this.$refs.seller_ref.offsetWidth / 100 * 3.6\n      // ÂíåÂàÜËæ®ÁéáÂ§ßÂ∞èÁõ∏ÂÖ≥ÁöÑÈÖçÁΩÆÈ°π\n      const adapterOption = {\n        title: {\n          textStyle: {\n            fontSize: titleFontSize\n          }\n        },\n        tooltip: {\n          axisPointer: {\n            lineStyle: {\n              width: titleFontSize\n            }\n          }\n        },\n        series: [\n          {\n            barWidth: titleFontSize,\n            itemStyle: {\n              barBorderRadius: [0, titleFontSize / 2, titleFontSize / 2, 0]\n            }\n          }\n        ]\n      }\n      this.chartInstance.setOption(adapterOption)\n      // ÊâãÂä®ÁöÑË∞ÉÁî®ÂõæË°®ÂØπË±°ÁöÑresize ÊâçËÉΩ‰∫ßÁîüÊïàÊûú\n      this.chartInstance.resize()\n    }\n  },\n  computed: {\n    ...mapState(['theme'])\n  },\n  watch: {\n    theme () {\n      console.log('‰∏ªÈ¢òÂàáÊç¢‰∫Ü')\n      this.chartInstance.dispose() // ÈîÄÊØÅÂΩìÂâçÁöÑÂõæË°®\n      this.initChart() // ÈáçÊñ∞‰ª•ÊúÄÊñ∞ÁöÑ‰∏ªÈ¢òÂêçÁß∞ÂàùÂßãÂåñÂõæË°®ÂØπË±°\n      this.screenAdapter() // ÂÆåÊàêÂ±èÂπïÁöÑÈÄÇÈÖç\n      this.updateChart() // Êõ¥Êñ∞ÂõæË°®ÁöÑÂ±ïÁ§∫\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n</style>\n"]}]}